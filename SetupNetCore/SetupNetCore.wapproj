<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="..\shared.props" />
  <PropertyGroup>
    <VisualStudioVersion Condition="'$(VisualStudioVersion)' == '' or '$(VisualStudioVersion)' &lt; '15.0'">15.0</VisualStudioVersion>
    <WapProjPath Condition="'$(WapProjPath)'==''">$(MSBuildExtensionsPath)\Microsoft\DesktopBridge\</WapProjPath>
  </PropertyGroup>
  <ItemGroup Label="ProjectConfigurations">
    <ProjectConfiguration Include="Debug|x86">
      <Configuration>Debug</Configuration>
      <Platform>x86</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Release|x86">
      <Configuration>Release</Configuration>
      <Platform>x86</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Debug|x64">
      <Configuration>Debug</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Release|x64">
      <Configuration>Release</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
  </ItemGroup>
  <Import Project="$(WapProjPath)\Microsoft.DesktopBridge.props" />
  <PropertyGroup>
    <ProjectGuid>83494626-246d-4df0-871a-22d1ce681b99</ProjectGuid>
    <TargetPlatformVersion>$(TPV)</TargetPlatformVersion>
    <TargetPlatformMinVersion>$(MinTPV)</TargetPlatformMinVersion>
    <DefaultLanguage>en-US</DefaultLanguage>
    <PackageCertificateKeyFile>SetupNetCore_TemporaryKey.pfx</PackageCertificateKeyFile>
    <EntryPointProjectUniqueName>..\DevPrompt\DevPromptNetCore.csproj</EntryPointProjectUniqueName>
    <GenerateAppInstallerFile>False</GenerateAppInstallerFile>
    <AppxAutoIncrementPackageRevision>True</AppxAutoIncrementPackageRevision>
    <AppxBundlePlatforms>x86;x64</AppxBundlePlatforms>
    <AppInstallerUpdateFrequency>1</AppInstallerUpdateFrequency>
    <AppInstallerCheckForUpdateFrequency>OnApplicationRun</AppInstallerCheckForUpdateFrequency>
    <AppxBundle>Always</AppxBundle>
    <ConfigOutRoot>$(OutRoot)$(Configuration).$(Platform)\</ConfigOutRoot>
    <ProjectOutRoot>$(ConfigOutRoot)$(MSBuildProjectName)\</ProjectOutRoot>
    <IntermediateOutputPath>$(ProjectOutRoot)</IntermediateOutputPath>
    <OutputPath>$(ConfigOutRoot)bin\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <AppxManifest Include="Package.appxmanifest">
      <SubType>Designer</SubType>
    </AppxManifest>
    <None Include="SetupNetCore_TemporaryKey.pfx" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="..\out\$(Configuration).x64\bin\DevInject64.dll">
      <Link>DevPromptNetCore\DevInject64.dll</Link>
    </Content>
    <Content Include="..\out\$(Configuration).x64\bin\DevInjector64.exe">
      <Link>DevPromptNetCore\DevInjector64.exe</Link>
    </Content>
    <Content Include="..\out\$(Configuration).x64\bin\DevNative64.dll">
      <Link>DevPromptNetCore\DevNative64.dll</Link>
    </Content>
    <Content Include="..\out\$(Configuration).x86\bin\DevInject32.dll">
      <Link>DevPromptNetCore\DevInject32.dll</Link>
    </Content>
    <Content Include="..\out\$(Configuration).x86\bin\DevInjector32.exe">
      <Link>DevPromptNetCore\DevInjector32.exe</Link>
    </Content>
    <Content Include="..\out\$(Configuration).x86\bin\DevNative32.dll">
      <Link>DevPromptNetCore\DevNative32.dll</Link>
    </Content>
    <Content Include="DevPromptNetCore\SplashScreen.scale-200.png" />
    <Content Include="DevPromptNetCore\LockScreenLogo.scale-200.png" />
    <Content Include="DevPromptNetCore\Square150x150Logo.scale-200.png" />
    <Content Include="DevPromptNetCore\Square44x44Logo.scale-200.png" />
    <Content Include="DevPromptNetCore\Square44x44Logo.targetsize-24_altform-unplated.png" />
    <Content Include="DevPromptNetCore\StoreLogo.png" />
    <Content Include="DevPromptNetCore\Wide310x150Logo.scale-200.png" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DevPrompt\DevPromptNetCore.csproj">
      <DesktopBridgeSelfContained>True</DesktopBridgeSelfContained>
      <DesktopBridgeIdentifier>$(DesktopBridgeRuntimeIdentifier)</DesktopBridgeIdentifier>
      <Properties>SelfContained=%(DesktopBridgeSelfContained);RuntimeIdentifier=%(DesktopBridgeIdentifier)</Properties>
      <SkipGetTargetFrameworkProperties>True</SkipGetTargetFrameworkProperties>
    </ProjectReference>
  </ItemGroup>
  <Import Project="$(WapProjPath)\Microsoft.DesktopBridge.targets" />
</Project>